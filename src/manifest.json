{
  "manifest_version": 2,
  "name": "YouTube Endcard Controller",
  "version": "1.0.0",
  "description": "Hide or show endcards on YouTube",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
